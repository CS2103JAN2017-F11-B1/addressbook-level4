<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox id="cardPane" fx:id="cardPane" alignment="CENTER_LEFT" prefHeight="25.0">
          <children>
                  <HBox alignment="CENTER_LEFT" prefWidth="172.0" spacing="5">
                      <children>
                          <HBox alignment="CENTER_LEFT">
                              <Label fx:id="id" alignment="CENTER" prefWidth="25.0" styleClass="cell_big_label" />
                              <Label fx:id="name" styleClass="cell_big_label" text="\$first" />
                          </HBox>
                      </children>
                  </HBox>
                  <Label fx:id="startTime" prefWidth="86.0" styleClass="cell_small_label" text="\$start time" />
                  <Label fx:id="endTime" prefWidth="85.0" styleClass="cell_small_label" text="\$end time" />
          </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER_LEFT" prefHeight="20.0" prefWidth="200.0">
         <children>
            <Label prefWidth="27.0" />
                  <Label fx:id="description" maxWidth="250.0" prefHeight="20.0" prefWidth="200.0" styleClass="cell_small_label" text="\$description" />
                  <FlowPane fx:id="tags" alignment="CENTER_LEFT" prefHeight="20.0" prefWidth="100.0" />
         </children>
      </HBox>
   </children>
</VBox>
